            </wsdl:input>
            <wsdl:output>
                <soap12:body use="literal" />
            </wsdl:output>
        </wsdl:operation>
    </wsdl:binding>
    <wsdl:service name="Stub">
        <wsdl:port name="StubSoap" binding="tns:StubSoap">
            <soap:address location="http://silentnight.staging.space48.com/bpf/soap/update" />
        </wsdl:port>
        <wsdl:port name="StubSoap12" binding="tns:StubSoap12">
